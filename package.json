    "init:all": "node .\\npm_scripts\\createMongodbDirectory && node .\\npm_scripts\\mongodConfig && node .\\npm_scripts\\initMongodb",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|less)$": "identity-obj-proxy"
    }
  },
