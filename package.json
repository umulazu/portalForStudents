    "build": "webpack --progress --config .\\webpack\\production.config.js & babel .\\server --out-dir .\\dist\\server",
    "prod": "npm run build & node .\\dist\\index.js --mode=production",
    "start:client": "webpack-dev-server --progress --config .\\webpack\\development.config.js --watch --hotOnly",
